"model": {
  <% for modelName, model of @models: %>
    "<%= modelName %>": {
      "name": "<%= modelName %>",
      "property": {
        <% for propName, p of model.properties: %>
          "<%= propName %>": "<%= propName %>",
        <% end %>
        "__dummy": ""
      },
      "relation": {
        <% for relName, relDef of model.relations: %>
          "<%= relName %>": "<%= relName %>",
        <% end %>
        "__dummy": ""
      }
    },
  <% end %>
  "__dummy": ""
}
